<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='res' type='com.eyespynature.user.Resources' />

    <ui:style>
        .panel {
        	border: 2px solid green;
        	padding: 2px;
        	background: beige;
        }
        
        .smallright {
        	margin: 0px 10px 0px 0px;
        }
    </ui:style>

    <g:HorizontalPanel addStyleNames='{style.panel}'>
        <g:InlineHyperlink targetHistoryToken="basket" addStyleNames='{style.smallright}'>Basket
        </g:InlineHyperlink>
        <g:HTML ui:field="contents"></g:HTML>

    </g:HorizontalPanel>

</ui:UiBinder>