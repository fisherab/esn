<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style>
		.main {
			margin: 40px 0px 0px 40px;
		}
		
		.label {
			margin: 4px 0px 0px 40px;
		}
	</ui:style>

	<g:HorizontalPanel addStyleNames='{style.main}'>
		<g:Button ui:field="previous">Previous</g:Button>
		<g:Button ui:field="down2" />
		<g:Button ui:field="down1" />
		<g:Button ui:field="here" />
		<g:Button ui:field="up1" />
		<g:Button ui:field="up2" />
		<g:Button ui:field="next">Next</g:Button>
		<g:Label ui:field="countBox" addStyleNames='{style.label}'></g:Label>
	</g:HorizontalPanel>

</ui:UiBinder>