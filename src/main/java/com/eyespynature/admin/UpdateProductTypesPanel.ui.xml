<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:a="urn:import:com.eyespynature.admin"
	xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

	<g:VerticalPanel>
		<g:HTML>
			<h2 ui:field="title" />
		</g:HTML>
		<g:HTML ui:field="status"></g:HTML>

		<c:CellTable ui:field="table"></c:CellTable>
		<c:SimplePager ui:field="pager" location="CENTER"></c:SimplePager>

		<a:ApplyDiscardPanel ui:field="adp" />
		<g:Button ui:field="reindex">Reindex</g:Button>

	</g:VerticalPanel>


</ui:UiBinder>